import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);
        int n = sc.nextInt();
        sc.nextLine();
        String t = sc.nextLine();
        char c =sc.next().charAt(0);
        System.out.println(n);
        System.out.println(t);
        System.out.println(c);
    }
}
